{
    "base": {
        "app-id": "io.github.Dretch.MonomerFlatpakExample",
        "runtime": "org.freedesktop.Platform",
        "runtime-version": "22.08",
        "sdk": "org.freedesktop.Sdk",
        "command": "monomer-flatpak-example",
        "finish-args": [
            "--socket=fallback-x11",
            "--socket=wayland",
            "--share=ipc",
            "--device=dri"
        ],
        "cleanup": [
            "/bin/alex",
            "/bin/c2hs",
            "/bin/dev-test-app",
            "/bin/happy",
            "/share/*-ghc-*/alex-*",
            "/share/*-ghc-*/c2hs-*",
            "/share/*-ghc-*/happy-*",
            "/share/*-ghc-*/sdl2-*"
        ],
        "cleanup-commands": [],
        "modules": [
            "extra-libraries/glu.json",
            "extra-libraries/glew.json",
            "extra-libraries/alex.json",
            "extra-libraries/happy.json"
        ]
    },
    "main-package": "monomer-flatpak-example"
}